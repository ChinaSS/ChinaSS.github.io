<form id="newsArtistRelFormId">
          <div class="row">
              <div class="col-md-6 form-horizontal">
                  <div class="form-group">
                      <label for="newsTitle" class="col-md-4 col-sm-2 control-label"><span style="color:red">*</span>新闻标题:</label>
                      <div class="col-md-8 col-sm-10">
                          <input id="newsTitle" name="newsTitle" type="text" class="form-control" cs-dialog="dialogNewsList.initConfig" ng-model="artistInfo.tempNewsArtistRel.newsTitle" >
                      </div>
                  </div>
              </div>
	          <div class="col-md-6 form-horizontal">
                <div class="form-group">
                    <label for="newsTypeName" class="col-md-4 col-sm-2 control-label">新闻类别:</label>
                    <div class="col-md-8 col-sm-10">
                        <input id="newsTypeName" name="newsTypeName" type="text" class="form-control" ng-model="artistInfo.tempNewsArtistRel.newsTypeName" readonly>
                    </div>
                </div>
              </div>
		</div>
        <div class="row">
               <div class="col-md-6 form-horizontal">
                  <div class="form-group">
                      <label  for="source" class="col-md-4 col-sm-2 control-label">来源:</label>
	                   <div class="col-md-8 col-sm-10">
	                        <input id="source" name="source" type="text" class="form-control" ng-model="artistInfo.tempNewsArtistRel.source" readonly>
	                   </div>
                  </div>
              </div>
             <div class="col-md-6 form-horizontal">
                <div class="form-group">
                    <label for="summary" class="col-md-4 col-sm-2 control-label">摘要:</label>
                    <div class="col-md-8 col-sm-10">
                        <input id="summary" name="summary" type="text" class="form-control" ng-model="artistInfo.tempNewsArtistRel.summary" readonly>
                    </div>
                </div>
            </div>
	    </div>
</form>
	    