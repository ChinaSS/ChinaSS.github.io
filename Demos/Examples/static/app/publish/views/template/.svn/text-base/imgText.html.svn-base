<div class="imgText" style="width: {{width}}px">
    <div class="tbStyle">
        <!--上下方式：标题、图片、新闻列表-->
        <div>
            <a target="_blank" href="{{imgLink}}">
                <img src="{{imgUrl}}" style="height: {{imgHeight}}px; width: {{width}}px"/>
            </a>
        </div>
        <ul>
        {{#news}}
            <li><a target="_blank" href="{{newsLink}}">{{title}}</a></li>
        {{/news}}
        </ul>
    </div>
</div>